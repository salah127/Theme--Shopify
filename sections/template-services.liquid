									{{ block.settings.note }}</div>
							{% endif %}
						</div>
					</div>
				</div>
			{% endif %}
		{% endfor %}
	</div>
</div>

{% schema %}
{
	"name": "Services",
	"class": "section section_template section_template__services",
	"max_blocks": 8,
	"settings": [
		{
			"type": "header",
			"content": "«Services» page"
		},
		{
			"type": "text",
			"id": "delivery_title",
			"label": "Delivery title"
		}
	],

	"blocks": [

		// SERVICE BLOCK /////////////////////////////////////////////////////////////////////////////////////
		{
			"type": "service-block",
			"name": "Service block",
			"settings": [
				{
					"type": "select",
					"id": "icon",
					"label": "Icon",
					"options": [
						{
							"value": "wallet",
							"label": "Wallet"
						},
						{
							"value": "tags",
							"label": "Tags"
						},
						{
							"value": "bag",
							"label": "Bag"
						},
						{
							"value": "man-woman",
							"label": "Man Woman"
						},
						{
							"value": "binoculars",
							"label": "Binoculars"
						},
						{
							"value": "equalizer",
							"label": "Equalizer"
						},
						{
							"value": "store",
							"label": "Store"
						}
					],
					"default": "store"
				},
				{
					"type": "text",
					"id": "title",
					"label": "Title"
				},
				{
					"type": "richtext",
					"id": "text",
					"label": "Text"
				}
			]
		},


		// DELIVERY BLOCK /////////////////////////////////////////////////////////////////////////////////////
		{
			"type": "delivery",
			"name": "Delivery block",
			"settings": [
				{
					"type": "text",
					"id": "title",
					"label": "Title",
					"default": "Delivery"
				},
				{
					"type": "image_picker",
					"id": "image",
					"label": "Image size 470x338"
				},
				{
					"type": "richtext",
					"id": "text",
					"label": "Text"
				},
				{
					"type": "text",
					"id": "note",
					"label": "Note text"
				}
			]
		}

	]

}
{% endschema %}