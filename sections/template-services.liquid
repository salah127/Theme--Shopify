						{% endcase %}	

						{{ item_icon }}

						{% if block.settings.title.size > 0 %}
							<h3>{{ block.settings.title }}</h3>
						{% endif %}

						{% if block.settings.text.size > 0 %}
							<div class="service_text">{{ block.settings.text }}</div>
						{% endif %}
					</div>
				{% endif %}
			{% endfor %}
		</div>

		{% for block in section.blocks %}
			{% if block.type == 'delivery' %}
				<div class="delivery_item">
					<h2 class="page_heading">{{ block.settings.title }}</h2>
					<div class="delivery_item_content">
						<div class="item_image">
							{% if block.settings.image %}
								<img src="{{ block.settings.image | img_url: '470x338', crop: 'center' }}" alt="{{ block.settings.title }}">
							{% else %}
								{{ 'image' | placeholder_svg_tag: 'img_placeholder' }}
							{% endif %}
						</div>
						<div class="item_text">
							<div class="text_content">{{ block.settings.text }}</div>
							{% if block.settings.note.size > 0 %}
								<div class="note_text">
